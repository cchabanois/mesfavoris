<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="mesfavoris.bookmarkType">
      <gotoBookmark
            class="mesfavoris.java.GotoInsideJavaElementBookmark"
            priority="10">
      </gotoBookmark>
      <propertiesProvider
            class="mesfavoris.java.editor.JavaEditorBookmarkPropertiesProvider"
            priority="10">
      </propertiesProvider>
      <propertiesProvider
            class="mesfavoris.java.element.JavaTypeMemberBookmarkPropertiesProvider"
            priority="10">
      </propertiesProvider>
      <labelProvider
            class="mesfavoris.java.JavaBookmarkLabelProvider"
            priority="10">
      </labelProvider>
      <markerAttributesProvider
            class="mesfavoris.java.JavaBookmarkMarkerAttributesProvider"
            priority="10">
      </markerAttributesProvider>
      <locationProvider
            class="mesfavoris.java.JavaTypeMemberBookmarkLocationProvider"
            priority="10">
      </locationProvider>
      <locationProvider
            class="mesfavoris.java.JavaMarkerBookmarkLocationProvider"
            priority="-1">
      </locationProvider>
      <gotoBookmark
            class="mesfavoris.java.GotoJavaBookmarkMarker"
            priority="10">
      </gotoBookmark>
   </extension>

</plugin>
